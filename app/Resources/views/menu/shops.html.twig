{%  extends 'menu/layout.html.twig' %}

{% block content %}
    <div class="shops_list">
        {% for shop in shops %}
            <div class="col-md-4">
                <img src="{{ asset('' ~ shop.getImageLocation() ~ '') }}" class="img-thumbnail" .img-responsive>
                <a href="shops/{{ shop.getSlug() }}">{{ shop.getName() }}</a>
            </div>
        {% endfor %}
    </div>
{% endblock %}